{
    "networking": {
        "main_cu": "localhost",
        "raspi": "localhost"
    },
    "ros": {
        "msg_queue_size": 10,
        "nodes": {
            "main_cu": [
                "camera_node", "keyboard_node", "serial_interface_node", "joystick_node"
            ],
            "raspi": []
        },
        "ignore_machines": [],
        "ignore_nodes": []
    },
    "system": {
        "debug": true,
        "allowed_log_levels": [
            "ERROR",
            "INFO",
            "DEBUG"
        ]
    },
    "security": {
        "main_cu": {
            "username": "machine_ssh_username",
            "password": "machine_ssh_password"
        },
        "raspi": {
            "username": "machine_ssh_username",
            "password": "machine_ssh_password"
        }
    }
}
